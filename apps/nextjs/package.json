{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "dev:test": "pnpm with-test-env next dev",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "start:local": "pnpm with-env next start",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-test-env": "dotenv -e ../../.env.test.local --",
    "test:e2e": "pnpm dlx playwright test --project=chromium"
  },
  "dependencies": {
    "@acme/api": "^0.1.0",
    "@acme/db": "^0.1.0",
    "@acme/logger": "^0.0.1",
    "@acme/shared": "^0.1.0",
    "@mantine/core": "6.0.13",
    "@mantine/form": "6.0.13",
    "@mantine/hooks": "6.0.13",
    "@mantine/modals": "6.0.13",
    "@mantine/next": "6.0.13",
    "@mantine/notifications": "6.0.13",
    "@mantine/utils": "6.0.13",
    "@t3-oss/env-nextjs": "^0.4.0",
    "@tabler/icons-react": "2.20.0",
    "@tanstack/react-query": "^4.29.12",
    "@trpc/client": "^10.28.1",
    "@trpc/next": "^10.28.1",
    "@trpc/react-query": "^10.28.1",
    "@trpc/server": "^10.28.1",
    "i18next": "22.5.0",
    "i18next-resources-to-backend": "1.1.4",
    "lodash": "4.17.21",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "next-i18next": "13.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.3.1",
    "superjson": "1.12.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.1.0",
    "@acme/tailwind-config": "^0.1.0",
    "@emotion/react": "11.11.0",
    "@emotion/server": "11.11.0",
    "@playwright/test": "1.34.3",
    "@types/lodash": "4.14.195",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}
